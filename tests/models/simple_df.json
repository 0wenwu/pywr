                    "pandas_kwargs": {
                        "index_col": "node"
                    }
                }
            },
            "cost": {
                "type": "constant",
                "value": {
                    "url": "simple_data.csv",
                    "column": "cost",
                    "index": "demand1",
                    "pandas_kwargs": {
                        "index_col": "node"
                    }
                }
            }
        }
    ],
    "edges": [
        ["supply1", "link1"],
        ["link1", "demand1"]
    ]
